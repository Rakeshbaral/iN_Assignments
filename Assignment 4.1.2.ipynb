    "def filter_long_words(n,list2):\n",
    "    list3=[]\n",
    "    for word in list2:\n",
    "        if(len(word)>n):\n",
    "            list3.append(word)\n",
    "    return list3\n",
    "\n",
    "list2=['to','the','from','is','face']\n",
    "list3=filter_long_words(2,list2)\n",
    "print(list3)"

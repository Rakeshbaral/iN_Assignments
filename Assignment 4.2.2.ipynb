   "source": [
    "def check_vowels(character):\n",
    "    vowel=['a','e','i','o','u']\n",
    "    #flag=False\n",
    "    for i in vowel:\n",
    "        if(character==i):\n",
    "            return True\n",
    "    return False\n",
    "\n",
    "character=input('Enter a letter to check if it is a vowel or not')\n",
    "print(check_vowels(character))\n"
